{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true
    },
    "exclude": ["node_modules", "dist", "ui"]
}
